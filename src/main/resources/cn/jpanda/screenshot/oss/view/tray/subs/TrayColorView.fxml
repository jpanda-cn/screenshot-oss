            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/color/blue.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="green" onMouseClicked="#green">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/color/green.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="black" onMouseClicked="#black">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/color/black.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="white" onMouseClicked="#white">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/color/white.png"/>
            </ImageView>
        </graphic>
    </Button>
</HBox>
