<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefWidth="380.0" style="-fx-background-color: rgb(224,224,224);" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.jpanda.screenshot.oss.view.tray.CanvasCutTrayView">
    <Button fx:id="drag" layoutX="10.0" layoutY="15.0" mnemonicParsing="false" onMouseClicked="#doDrag"
            prefHeight="35.0" prefWidth="35.0">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/drag.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="roundness" layoutX="55.0" layoutY="15.0" mnemonicParsing="false" onMouseClicked="#doRoundness"
            prefHeight="35.0" prefWidth="35.0">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/roundness.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="rectangle" layoutX="100.0" layoutY="15.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0"
            onMouseClicked="#doRectangle">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/rectangle.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="arrow" layoutX="145.0" layoutY="15.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" onMouseClicked="#doArrow">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/arrow.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="pen" layoutX="190.0" layoutY="15.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" onMouseClicked="#doPen">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/pen.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="text" layoutX="235.0" layoutY="15.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" onMouseClicked="#doText">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/text.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="cancel" layoutX="280.0" layoutY="15.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/cancel.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="submit" layoutX="325.0" layoutY="15.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/submit.png"/>
            </ImageView>
        </graphic>
    </Button>
    <AnchorPane fx:id="bar" layoutX="4.0" layoutY="64.0">
    </AnchorPane>
    <Separator layoutX="14.0" layoutY="58.0" visible="false"/>
</AnchorPane>
