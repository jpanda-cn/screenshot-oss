<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.Image?>
<AnchorPane prefHeight="400.0" prefWidth="336.0" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="cn.jpanda.screenshot.oss.view.image.GitFileImageStoreConfig">
    <ImageView fitHeight="87.0" fitWidth="308.0" layoutX="14.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
        <Image url="/images/logo.png"/>
    </ImageView>
    <Label layoutX="27.0" layoutY="130.0" text="本地仓库地址"/>
    <Label layoutX="27.0" layoutY="160.0" text="图片子目录地址"/>
    <Label layoutX="27.0" layoutY="191.0" text="远程仓库地址"/>
    <Label layoutX="28.0" layoutY="221.0" text="分支名称"/>
    <Label layoutX="27.0" layoutY="250.0" text="用户名"/>
    <Label layoutX="27.0" layoutY="280.0" text="密码"/>
    <Label layoutX="26.0" layoutY="312.0" text="异步上传"/>
    <TextField fx:id="localRepositoryDir" layoutX="117.0" layoutY="127.0"/>
    <TextField fx:id="subDir" layoutX="117.0" layoutY="157.0"/>
    <TextField fx:id="remoteRepositoryUrl" layoutX="117.0" layoutY="188.0"/>
    <TextField fx:id="branch" layoutX="117.0" layoutY="218.0"/>
    <TextField fx:id="username" layoutX="117.0" layoutY="246.0"/>
    <PasswordField fx:id="password" layoutX="117.0" layoutY="275.0"/>
    <CheckBox fx:id="async" layoutX="117.0" layoutY="312.0" mnemonicParsing="false"/>
    <Button fx:id="cancel" layoutX="91.0" layoutY="347.0" mnemonicParsing="false" text="取消" onMouseClicked="#cancel"/>
    <Button fx:id="save" layoutX="227.0" layoutY="347.0" mnemonicParsing="false" text="保存" onMouseClicked="#save"/>
    <Button fx:id="choseLocalRepositoryDir" layoutX="288.0" layoutY="128.0" mnemonicParsing="false" prefHeight="23.0"
            prefWidth="36.0" text="..." onMouseClicked="#choseLocalRepositoryDir"/>
</AnchorPane>
