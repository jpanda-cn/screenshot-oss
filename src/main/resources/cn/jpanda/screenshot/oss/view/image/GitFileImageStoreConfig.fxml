        <Label  layoutY="280.0" text="密码"/>
        <Label  layoutY="312.0" text="异步上传"/>
    </AnchorPane>
   <AnchorPane layoutX="109.0">
       <TextField fx:id="localRepositoryDir" layoutY="127.0"/>
       <TextField fx:id="subDir"  layoutY="157.0"/>
       <TextField fx:id="remoteRepositoryUrl"  layoutY="188.0"/>
       <TextField fx:id="branch"  layoutY="218.0"/>
       <TextField fx:id="username"  layoutY="246.0"/>
       <PasswordField fx:id="password"  layoutY="275.0"/>
       <CheckBox fx:id="async"  layoutY="312.0" mnemonicParsing="false"/>
   </AnchorPane>
   <AnchorPane>
       <Button fx:id="choseLocalRepositoryDir" layoutX="325.0" layoutY="128.0" mnemonicParsing="false"
               onMouseClicked="#choseLocalRepositoryDir" prefHeight="23.0" prefWidth="36.0" text="..."/>
   </AnchorPane>
</AnchorPane>
