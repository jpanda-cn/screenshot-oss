<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="235.0" prefWidth="319.0" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.jpanda.screenshot.oss.view.main.CutView">
    <ImageView fitHeight="87.0" fitWidth="308.0" layoutX="6.0" layoutY="34.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@logo.png"/>
    </ImageView>
    <Button layoutX="40.0" layoutY="121.0" mnemonicParsing="false" onKeyPressed="#doKeyCut" onMouseClicked="#doCut"
            prefHeight="87.0" prefWidth="238.0" text="截图" textFill="#49acee">
        <font>
            <Font name="System Bold" size="38.0"/>
        </font>
    </Button>
    <Button fx:id="toSettings" layoutX="275.0" layoutY="4.0" mnemonicParsing="false" text="设置" onMouseClicked="#toSettings" onKeyPressed="#toKeySettings"/>
</AnchorPane>
