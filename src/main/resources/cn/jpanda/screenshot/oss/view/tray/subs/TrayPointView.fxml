<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="cn.jpanda.screenshot.oss.view.tray.subs.TrayPointView">
    <Button fx:id="smallPoint" onMouseClicked="#small">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/point/smallpoint.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="midpoint" onMouseClicked="#mid">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/point/midpoint.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="bigpoint" onMouseClicked="#big">
        <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0">
                <Image url="@/images/tray/point/bigpoint.png"/>
            </ImageView>
        </graphic>
    </Button>
</HBox>
