<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="160.0" prefWidth="130.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.jpanda.screenshot.oss.view.tray.subs.TrayRGBView">
    <ImageView fx:id="image" fitHeight="130.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true"/>
    <VBox layoutY="130.0" prefHeight="30" prefWidth="130">
        <Text fx:id="pos" strokeType="OUTSIDE" strokeWidth="0.0" text="POS:(1024,2048)">
            <font>
                <Font size="10.0"/>
            </font>
        </Text>
        <Text fx:id="rgba" strokeType="OUTSIDE" strokeWidth="0.0" text="RGBA:(125,222,223,0.4)">
            <font>
                <Font size="10.0"/>
            </font>
        </Text>
        <Text fx:id="hex" strokeType="OUTSIDE" strokeWidth="0.0" text="HEX:(#FFFFFF)">
            <font>
                <Font size="10.0"/>
            </font>
        </Text>
    </VBox>
</AnchorPane>
